{
    "name": "zaichaopan/dynamic-breadcrumbs",
    "description": "A package to add dynamic breadcrumbs to your laravel app",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zaichaopan",
            "email": "zaichaopan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zaichaopan\\Breadcrumb\\": "src"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/auth": "~5.5.0|~5.6.0",
        "illuminate/container": "~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5.0",
        "phpunit/phpunit": "^7.2",
        "mockery/mockery": "^1.1",
        "illuminate/routing": "~5.5.0|~5.6.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/Category.php",
            "tests/stubs/Product.php",
            "tests/stubs/ProductsController.php"
        ]
    },
    "minimum-stability": "stable"
}
