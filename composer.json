{
    "name": "zaichaopan/breadcrumb",
    "description": "A package to add dynamic breadcrumb to your laravel app",
    "type": "library",
    "keywords": [
        "laravel",
        "breadcrumb"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "zaichaopan",
            "email": "zaichaopan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zaichaopan\\Breadcrumb\\": "src"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5.0",
        "phpunit/phpunit": "^7.2",
        "mockery/mockery": "^1.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/Category.php",
            "tests/stubs/Product.php",
            "tests/stubs/ProductsController.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaichaopan\\breadcrumb\\BreadcrumbServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
